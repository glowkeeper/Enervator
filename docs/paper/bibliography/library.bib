  date = {1975-11-01},
  pages = {253-259},
  keywords = {Carbonic Anhydrases,Deuterium,Humans,Hydrogen-Ion Concentration,Isoenzymes,Kinetics,Mathematics,Water},
  author = {Steiner, H and Jonsson, B H and Lindskog, S},
  note = {tex.eprint: 1249
