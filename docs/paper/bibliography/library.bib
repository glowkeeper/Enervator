@article{gottmann_why_1966,
  title = {'{{Why}} the {{Skyscraper}}?},
  volume = {56},
  issn = {00167428},
  doi = {10.2307/212878},
  abstract = {As an architectural style the skyscraper appears to have functional advantages because of present changes in the occupational distribution of the labor force and because of the strong interdependence developing between the most rapidly growing sectors of employment. The "white-collar revolution," agglomerating high densities of offices and related activities, requires the rise of skyscrapers. This new "gothic style" of the city center is compared to the function and meaning of the medieval cathedral, with its vast stained-glass windows. Abstract and artistic concepts are involved that may well be related to the contemporary evolution of the community and of society.},
  number = {2},
